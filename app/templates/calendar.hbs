<div class="container">
	<div class="row">
		
		<!-- Calendar -->
		<div class="col-md-8">
			<div class="well"> <!--Rounded grey box -->
				<div class="row">
					<div class="col-xs-9 col-md-9">
					</div>
					<div class="col-xs-3 col-md-3">
						<div class="btn-group right-group" role="group" aria-label="Basic example">
							<button type="button" id="week_button" class="btn grey-button" >Week</button>
							<button type="button" id="month_button" class="btn grey-button btn-active">Month</button>
							<script>
								$('#week_button').click(function() {
									$('#calendar').fullCalendar('changeView', 'basicWeek');
									$(this).addClass('btn-active');
									$('#month_button').removeClass('btn-active');
								});
								$('#month_button').click(function() {
									$('#calendar').fullCalendar('changeView', 'month');
									$(this).addClass('btn-active');
									$('#week_button').removeClass('btn-active');
								});
							</script>
						</div>
					</div>
				</div>
				<div class="calendar-container">
					<!-- MSF Calendar (Main + Birthdays) -->
					
					{{full-calendar}}
				</div>

				<div class="row">
					<div class="col-xs-12 col-md-12">
						<div class="col-xs-5 col-md-5">
						</div>
						<div class="col-xs-6 col-md-4">
							<a href="https://calendar.google.com/calendar/ical/uscmsf%40gmail.com/public/basic.ics"><button type="button" class="btn grey-button">Export</button></a>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>